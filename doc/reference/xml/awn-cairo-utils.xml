<refsect2 id="AwnCairoRoundCorners" role="enum">
<title>enum AwnCairoRoundCorners</title>
<indexterm zone="AwnCairoRoundCorners"><primary sortas="AwnCairoRoundCorners">AwnCairoRoundCorners</primary></indexterm><programlisting>typedef enum
{
	ROUND_NONE		= 0,
	ROUND_TOP_LEFT		= 1,
	ROUND_TOP_RIGHT		= 2,
	ROUND_BOTTOM_RIGHT	= 4,
	ROUND_BOTTOM_LEFT	= 8,
	ROUND_TOP		= ROUND_TOP_LEFT | ROUND_TOP_RIGHT,
	ROUND_BOTTOM		= ROUND_BOTTOM_LEFT | ROUND_BOTTOM_RIGHT,
	ROUND_LEFT		= ROUND_TOP_LEFT | ROUND_BOTTOM_LEFT,
	ROUND_RIGHT		= ROUND_TOP_RIGHT | ROUND_BOTTOM_RIGHT,
	ROUND_ALL		= ROUND_LEFT | ROUND_RIGHT
} AwnCairoRoundCorners;
</programlisting>
<para>
