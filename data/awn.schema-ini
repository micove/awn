[DEFAULT/force_monitor]
type = bool
default = False
description = Don't use the monitor settings taken from GDK.
[DEFAULT/monitor_width]
type = int
default = 1024
description = The width of the monitor that overrides the GDK monitor settings, if force_monitor is TRUE.
[DEFAULT/monitor_height]
type = int
default = 768
description = The height of the monitor that overrides the GDK monitor settings, if force_monitor is TRUE.
[DEFAULT/monitor_xoffset]
type = int
default = 0
description = Allows the panel positioning to be offset by X, useful for dual monitor setups. If force_monitor is TRUE.
[DEFAULT/monitor_xalign]
type = float
default = 0.5
description = Set the panel alignment.
[DEFAULT/panel_mode]
type = bool
default = False
description = Sets the panel mode.  TRUE sets the panel to "3D" mode, FALSE sets the panel to "flat" mode.
[DEFAULT/auto_hide]
type = bool
default = False
description = Auto hides the dock when the cursor is not on the dock.
[DEFAULT/auto_hide_delay]
type = int
default = 1000
description = The time (in milliseconds) for the dock to wait to hide when the cursor leaves the dock.  Used only when auto_hide is TRUE.
[DEFAULT/keep_below]
type = bool
default = False
description = Always keep AWN below all other windows.
[DEFAULT/applets_list]
type = list-string
default = /usr/share/avant-window-navigator/applets/taskman.desktop::1
description = The list of paths to the enabled applets, in order from left to right.
[bar/rounded_corners]
type = bool
default = True
description = Enable/disable rounded corners on the dock bar.
[bar/corner_radius]
type = float
default = 10.0
description = The radius of the bar corners when rounded_corners is TRUE.
[bar/expand_bar]
type = bool
default = False
description = Expand the bar to the width of the monitor.
[bar/no_bar_resize_animation]
type = bool
default = False
description = Disable bar resize animations. Does not apply to the background animation.
[bar/render_pattern]
type = bool
default = False
description = Use a pattern image as the background of the bar.
[bar/pattern_uri]
type = string
default = ~
description = The path to the background pattern, if render_pattern is TRUE.
[bar/pattern_alpha]
type = float
default = 1.0
description = The opacity of the background pattern (between 0 and 1), if render_pattern is TRUE.
[bar/glass_step_1]
type = string
default = 454545C8
description = The first step color for the glass engine's gradient.
[bar/glass_step_2]
type = string
default = 010101BE
description = The second step color for the glass engine's gradient.
[bar/glass_histep_1]
type = string
default = FFFFFF0B
description = The first step color for the glass engine's highlighting.
[bar/glass_histep_2]
type = string
default = FFFFFF0A
description = The second step color for the glass engine's highlighting.
[bar/border_color]
type = string
default = 000000CC
description = The color of the bar's border.
[bar/hilight_color]
type = string
default = FFFFFF11
description = The color of the internal border of the bar.
[bar/show_separator]
type = bool
default = True
description = Show a separator between the launchers and the task manager.
[bar/sep_color]
type = string
default = FFFFFF00
description = The color of the separator between the launchers and the task manager, if show_separator is TRUE.
[bar/bar_height]
type = int
default = 48
description = The height of the dock bar, in pixels.
[bar/bar_angle]
type = int
default = 0
description = In 3D mode, the angle (in degrees, between 0 and 90) that the bar is tilted.
[bar/icon_offset]
type = int
default = 10
description = The number of pixels between the bottom of the icons and the bottom of the dock.
[bar/reflection_offset]
type = int
default = 0
description = The number of pixels the reflections are moved up.
[bar/bar_pos]
type = float
default = 0.5
description = The relative horizontal position of the bar.  0 is the left edge of the screen, 1 is the right edge.
[window_manager/show_all_windows]
type = bool
default = True
description = Show the tasks from every workspace in the bar.
[window_manager/launchers]
type = list-string
default = /usr/share/applications/awn-manager.desktop
description = A list of the paths to the launchers shown on the dock, in order from left to right.
[app/active_png]
type  = string
default = ~
description = Unused.
[app/use_png]
type = bool
default = False
description = Unused.
[app/arrow_color]
type = string
default = FFFFFF66
description = The color of the launcher arrows.
description[en_CA] = The colour of the launcher arrows.
description[en_GB] = The colour of the launcher arrows.
[app/arrow_offset]
type = int
default = 2
description = The offset between the arrows and the launchers.
[app/tasks_have_arrows]
type = bool
default = False
description = Determines whether tasks also have arrows beneath them.
[app/name_change_notify]
type = bool
default = False
description = Alert the user when the name of the window has changed.
[app/alpha_effect]
type = bool
default = False
description = Unused.
[app/icon_effect]
type = int
default = 0
description = Bitmask that stores which effects occur for each icon event type.
[app/hover_bounce_effect]
type = bool
default = False
description = Determines whether the bounce effect occurs when an icon is hovered over by the mouse.
[app/icon_alpha]
type = float
default = 1.0
description = Alpha value to apply to icons.
[app/reflection_alpha_multiplier]
type = float
default = 0.33
description = Reflection alpha is a multiple of the current alpha of the icon.
[app/frame_rate]
type = int
default = 25
description = Frame rate for awn effects.
[app/icon_depth_on]
type = bool
default = TRUE
description = Enable dept on 3d type effects.
[app/show_shadows]
type = bool
default = False
description = Shows shadows for the icons.
[title/text_color]
type = string
default = FFFFFFFF
description = The color of the title's text.
[title/shadow_color]
type = string
default = 1B3B12E1
description = The color of the shadow of the title.
[title/background]
type = string
default = 000000AA
description = The background color of the title.
[title/font_face]
type = string
default = Sans 11
description = The font details of the title.
